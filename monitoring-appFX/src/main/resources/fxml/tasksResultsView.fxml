<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="javafx.controllers.TasksResultsViewController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <TabPane>
            <Tab fx:id="table" text="Table view">
                <fx:include fx:id="tableView" source="tasksResultsTable.fxml"></fx:include>
            </Tab>
            <Tab fx:id="char" text="Chart view">
                <fx:include fx:id="chartView" source="tasksResultsChart.fxml"></fx:include>
            </Tab>
        </TabPane>
    </children>
</AnchorPane>
